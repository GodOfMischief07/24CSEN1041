x=input("create: ")
print("password created")
y=input("enter the password: ")
for i in range(0,3):
    if y==x:
        print("hello")
        break
    else:
        if i<2:
            y=input("re enter: ")
        else:
            print("too many attempts")

#OUTPUT
create: 2
password created
enter the password: 3
re enter: 4
re enter: 5
too many attempts
